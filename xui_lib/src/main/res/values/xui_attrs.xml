<?xml version="1.0" encoding="utf-8"?>

<!--控件的自定义属性-->
<resources>


    <!--自动换行-->
    <declare-styleable name="AutoNewLineLayout">
        <attr name="max_row" format="integer" />
        <attr name="max_column" format="integer" />
        <attr name="vertical_space" format="dimension" />
        <attr name="horizontal_space" format="dimension" />
    </declare-styleable>

    <!--ActionBar  TitleBar-->
    <declare-styleable name="TitleBar">
        <!--标题栏的高度，默认52dp-->
        <attr name="tb_barHeight" format="dimension" />
        <!--是否是沉浸式标题栏，默认是false-->
        <attr name="tb_immersive" format="boolean" />
        <!--左右图标和文字的水平方向padding-->
        <attr name="tb_action_horizontal_padding" format="dimension" />
        <!--左右图标和文字的垂直方向padding-->
        <attr name="tb_action_vertical_padding" format="dimension" />
        <!--标题栏两侧控件的padding-->
        <attr name="tb_sideTextPadding" format="dimension" />
        <!--标题栏左侧文字的颜色-->
        <attr name="tb_sideTextColor" format="color" />
        <!--标题栏标题文字的颜色-->
        <attr name="tb_titleTextColor" format="color" />
        <!--标题栏副标题文字的颜色-->
        <attr name="tb_subTitleTextColor" format="color" />
        <!---标题栏右侧文字的颜色-->
        <attr name="tb_actionTextColor" format="color" />
        <!--中间title的对齐方式-->
        <attr name="tb_centerGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
        <!--标题栏左侧文字的大小-->
        <attr name="tb_sideTextSize" format="dimension" />
        <!--标题栏标题文字的大小-->
        <attr name="tb_titleTextSize" format="dimension" />
        <!--标题栏副标题文字的大小-->
        <attr name="tb_subTitleTextSize" format="dimension" />
        <!--标题栏右侧文字的大小-->
        <attr name="tb_actionTextSize" format="dimension" />
        <!--文字的内容以及图标内容-->
        <attr name="tb_leftImageResource" format="reference" />
        <!--标题栏左侧的文字-->
        <attr name="tb_leftText" format="string" />
        <!--标题栏标题的文字-->
        <attr name="tb_titleText" format="string" />
        <!--标题栏副标题的文字-->
        <attr name="tb_subTitleText" format="string" />
        <!--标题栏分割线的颜色-->
        <attr name="tb_dividerColor" format="color" />
        <!--标题栏分割线的高度-->
        <attr name="tb_dividerHeight" format="dimension" />
    </declare-styleable>
    <attr name="TitleBarStyle" format="reference" />


    <!--数量选择器-->
    <declare-styleable name="NumberPicker">
        <!--中间view的宽度-->
        <attr name="middle_width" format="dimension" />
        <attr name="middle_text_size" format="dimension"></attr>
        <attr name="middle_text_color" format="color"></attr>
        <attr name="min_number" format="integer" />
        <attr name="max_number" format="integer" />
        <attr name="number_prefix" format="string" />
        <attr name="add_drawable" format="reference"></attr>
        <attr name="subtract_drawable" format="reference"></attr>
        <attr name="drawable_padding" format="dimension"></attr>
    </declare-styleable>


    <attr name="RadiusImageViewStyle" format="reference" />
    <declare-styleable name="RadiusImageView">
        <!--边框宽度，默认：0-->
        <attr name="riv_border_width" format="dimension" />
        <!--边框的颜色，默认：Color.GRAY-->
        <attr name="riv_border_color" format="color" />
        <!--选中时，覆盖在图片上的图层颜色，默认：Color.TRANSPARENT-->
        <attr name="riv_selected_mask_color" format="color" />
        <!--边框选中时的颜色，默认：Color.GRAY-->
        <attr name="riv_selected_border_color" format="color" />
        <!--边框选中时的宽度，默认：0-->
        <attr name="riv_selected_border_width" format="dimension" />
        <!--圆角的大小，默认0-->
        <attr name="riv_corner_radius" format="dimension" />
        <!--是否是椭圆形，默认：false-->
        <attr name="riv_is_oval" format="boolean" />
        <!--图片是否是圆形的，默认：false-->
        <attr name="riv_is_circle" format="boolean" />
        <!--图片是否可触摸选中，默认：true-->
        <attr name="riv_is_touch_select_mode_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ShapeTextView">
        <attr name="sGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="3" />
            <enum name="bottom" value="4" />
        </attr>

        <attr name="sShapeType" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="line" value="2" />
            <enum name="ring" value="3" />
        </attr>

        <attr name="sSolidColor" format="color" />

        <attr name="sSelectorPressedColor" format="color" />
        <attr name="sSelectorDisableColor" format="color" />
        <attr name="sSelectorNormalColor" format="color" />

        <attr name="sIsRadiusAdjustBounds" format="boolean" />

        <attr name="sCornersRadius" format="dimension" />

        <attr name="sCornersTopLeftRadius" format="dimension" />
        <attr name="sCornersTopRightRadius" format="dimension" />
        <attr name="sCornersBottomLeftRadius" format="dimension" />
        <attr name="sCornersBottomRightRadius" format="dimension" />

        <attr name="sStrokeWidth" format="dimension" />
        <attr name="sStrokeDashWidth" format="dimension" />
        <attr name="sStrokeDashGap" format="dimension" />
        <attr name="sStrokeColor" format="color" />

        <attr name="sSizeWidth" format="dimension" />
        <attr name="sSizeHeight" format="dimension" />

        <attr name="sGradientOrientation" format="enum">
            <enum name="TOP_BOTTOM" value="0" />
            <enum name="TR_BL" value="1" />
            <enum name="RIGHT_LEFT" value="2" />
            <enum name="BR_TL" value="3" />
            <enum name="BOTTOM_TOP" value="4" />
            <enum name="BL_TR" value="5" />
            <enum name="LEFT_RIGHT" value="6" />
            <enum name="TL_BR" value="7" />
        </attr>

        <attr name="sGradientAngle" format="dimension" />
        <attr name="sGradientCenterX" format="dimension" />
        <attr name="sGradientCenterY" format="dimension" />
        <attr name="sGradientGradientRadius" format="dimension" />
        <attr name="sGradientStartColor" format="color" />
        <attr name="sGradientCenterColor" format="color" />
        <attr name="sGradientEndColor" format="color" />
        <attr name="sGradientUseLevel" format="boolean" />

        <attr name="sUseSelector" format="boolean" />

        <attr name="sGradientType" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>

    </declare-styleable>


    <declare-styleable name="RadioGroupPlus">
        <attr name="rgp_checkedButton" format="reference"></attr>
    </declare-styleable>


    <declare-styleable name="OptionItem">

        <!-- 文字字符串 -->
        <attr name="sLeftTextString" format="string" />
        <attr name="sLeftTopTextString" format="string" />
        <attr name="sLeftBottomTextString" format="string" />

        <attr name="sCenterTextString" format="string" />
        <attr name="sCenterTopTextString" format="string" />
        <attr name="sCenterBottomTextString" format="string" />

        <attr name="sRightTextString" format="string" />
        <attr name="sRightTopTextString" format="string" />
        <attr name="sRightBottomTextString" format="string" />

        <!-- 文字颜色 -->
        <attr name="sLeftTextColor" format="color" />
        <attr name="sLeftTopTextColor" format="color" />
        <attr name="sLeftBottomTextColor" format="color" />

        <attr name="sCenterTextColor" format="color" />
        <attr name="sCenterTopTextColor" format="color" />
        <attr name="sCenterBottomTextColor" format="color" />

        <attr name="sRightTextColor" format="color" />
        <attr name="sRightTopTextColor" format="color" />
        <attr name="sRightBottomTextColor" format="color" />

        <!-- 字体大小 -->
        <attr name="sLeftTextSize" format="dimension" />
        <attr name="sLeftTopTextSize" format="dimension" />
        <attr name="sLeftBottomTextSize" format="dimension" />

        <attr name="sCenterTextSize" format="dimension" />
        <attr name="sCenterTopTextSize" format="dimension" />
        <attr name="sCenterBottomTextSize" format="dimension" />

        <attr name="sRightTextSize" format="dimension" />
        <attr name="sRightTopTextSize" format="dimension" />
        <attr name="sRightBottomTextSize" format="dimension" />

        <!-- 文字显示行数 -->
        <attr name="sLeftTopLines" format="integer" />
        <attr name="sLeftLines" format="integer" />
        <attr name="sLeftBottomLines" format="integer" />

        <attr name="sCenterTopLines" format="integer" />
        <attr name="sCenterLines" format="integer" />
        <attr name="sCenterBottomLines" format="integer" />

        <attr name="sRightTopLines" format="integer" />
        <attr name="sRightLines" format="integer" />
        <attr name="sRightBottomLines" format="integer" />
        <!-- 文字显示个数 -->

        <attr name="sLeftTopMaxEms" format="integer" />
        <attr name="sLeftMaxEms" format="integer" />
        <attr name="sLeftBottomMaxEms" format="integer" />

        <attr name="sCenterTopMaxEms" format="integer" />
        <attr name="sCenterMaxEms" format="integer" />
        <attr name="sCenterBottomMaxEms" format="integer" />

        <attr name="sRightTopMaxEms" format="integer" />
        <attr name="sRightMaxEms" format="integer" />
        <attr name="sRightBottomMaxEms" format="integer" />
        <!-- 文字对齐方式 -->
        <attr name="sLeftViewGravity" format="enum">
            <enum name="left_center" value="0" />
            <enum name="center" value="1" />
            <enum name="right_center" value="2" />
        </attr>
        <attr name="sCenterViewGravity" format="enum">
            <enum name="left_center" value="0" />
            <enum name="center" value="1" />
            <enum name="right_center" value="2" />
        </attr>
        <attr name="sRightViewGravity" format="enum">
            <enum name="left_center" value="0" />
            <enum name="center" value="1" />
            <enum name="right_center" value="2" />
        </attr>
        <!-- drawable资源 -->
        <attr name="sLeftTvDrawableLeft" format="reference" />
        <attr name="sLeftTvDrawableRight" format="reference" />

        <attr name="sCenterTvDrawableLeft" format="reference" />
        <attr name="sCenterTvDrawableRight" format="reference" />

        <attr name="sRightTvDrawableLeft" format="reference" />
        <attr name="sRightTvDrawableRight" format="reference" />

        <!--drawable的宽度-->
        <attr name="sLeftTvDrawableWidth" format="dimension" />
        <attr name="sLeftTvDrawableHeight" format="dimension" />

        <attr name="sCenterTvDrawableWidth" format="dimension" />
        <attr name="sCenterTvDrawableHeight" format="dimension" />

        <attr name="sRightTvDrawableWidth" format="dimension" />
        <attr name="sRightTvDrawableHeight" format="dimension" />

        <!--  TextView的drawable对应的Padding -->
        <attr name="sTextViewDrawablePadding" format="dimension" />
        <!-- 左边textView的宽度 为了中间文字左对齐的时候使用 -->
        <attr name="sLeftViewWidth" format="dimension" />
        <!-- 分割线的Margin -->
        <attr name="sTopDividerLineMarginLR" format="dimension" />
        <attr name="sTopDividerLineMarginLeft" format="dimension" />
        <attr name="sTopDividerLineMarginRight" format="dimension" />

        <attr name="sBottomDividerLineMarginLR" format="dimension" />
        <attr name="sBottomDividerLineMarginLeft" format="dimension" />
        <attr name="sBottomDividerLineMarginRight" format="dimension" />
        <!-- 分割线 -->
        <attr name="sDividerLineColor" format="color" />
        <attr name="sDividerLineHeight" format="dimension" />
        <attr name="sDividerLineType" format="enum">
            <enum name="none" value="0" />
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="both" value="3" />
        </attr>
        <attr name="sIsBottomDividerAlignLeftText" format="boolean"></attr>

        <!-- view的Margin -->
        <attr name="sLeftViewMarginLeft" format="dimension" />
        <attr name="sLeftViewMarginRight" format="dimension" />
        <attr name="sCenterViewMarginLeft" format="dimension" />
        <attr name="sCenterViewMarginRight" format="dimension" />
        <attr name="sRightViewMarginLeft" format="dimension" />
        <attr name="sRightViewMarginRight" format="dimension" />
        <!-- 文字是否加粗 -->
        <attr name="sLeftTopTextIsBold" format="boolean" />
        <attr name="sLeftTextIsBold" format="boolean" />
        <attr name="sLeftBottomTextIsBold" format="boolean" />

        <attr name="sCenterTopTextIsBold" format="boolean" />
        <attr name="sCenterTextIsBold" format="boolean" />
        <attr name="sCenterBottomTextIsBold" format="boolean" />

        <attr name="sRightTopTextIsBold" format="boolean" />
        <attr name="sRightTextIsBold" format="boolean" />
        <attr name="sRightBottomTextIsBold" format="boolean" />

        <attr name="sLeftTextBackground" format="reference" />
        <attr name="sCenterTextBackground" format="reference" />
        <attr name="sRightTextBackground" format="reference" />

        <!-- 图片资源 -->

        <attr name="sLeftIconRes" format="reference" />
        <attr name="sRightIconRes" format="reference" />

        <attr name="sLeftIconWidth" format="dimension" />
        <attr name="sLeftIconHeight" format="dimension" />

        <attr name="sRightIconWidth" format="dimension" />
        <attr name="sRightIconHeight" format="dimension" />

        <!--上中下三行文字的间距-->
        <attr name="sCenterSpaceHeight" format="dimension" />

        <attr name="sLeftIconMarginLeft" format="dimension" />
        <attr name="sRightIconMarginRight" format="dimension" />

        <attr name="sRightViewType" format="enum">
            <enum name="checkbox" value="0" />
            <enum name="switchBtn" value="1" />
            <enum name="radioButton" value="2" />
        </attr>

        <!--中间是否可编辑，默认false-->
        <attr name="sEnableEdit" format="boolean"/>
        <attr name="sEditBackGround" format="reference"/>
        <!--MATCH_PARENT=-1(默认), WRAP_CONTENT=-2-->
        <attr name="sEditTextWidth" format="dimension"/>
        <attr name="sEditTextString" format="string"/>
        <!--输入框的提示信息-->
        <attr name="sEditTextHint" format="string"/>

        <attr name="sEditTextGravity" format="integer">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
        </attr>

        <!--输入框的按钮类型,默认是clear-->
        <attr name="sEditTextButtonType" format="enum">
            <enum name="none" value="0" />
            <enum name="clear" value="1" />
            <enum name="password" value="2" />
        </attr>

        <attr name="android:inputType" />

        <!--CheckBox-->
        <attr name="sRightCheckBoxRes" format="reference" />
        <attr name="sRightCheckBoxMarginRight" format="dimension" />
        <attr name="sIsChecked" format="boolean" />

        <attr name="sRightSwitchMarginRight" format="dimension" />
        <attr name="sSwitchIsChecked" format="boolean" />

        <attr name="sRightRadioButtonRes" format="reference" />
        <attr name="sRightRadioButtonMarginRight" format="dimension" />
        <attr name="sRadioButtonId" format="reference" />


        <attr name="sTextOff" format="string" />
        <attr name="sTextOn" format="string" />

        <attr name="sSwitchMinWidth" format="dimension" />
        <attr name="sSwitchPadding" format="dimension" />
        <attr name="sThumbTextPadding" format="dimension" />

        <attr name="sThumbResource" format="reference" />
        <attr name="sTrackResource" format="reference" />

        <!--是否开启点击出现水波效果-->
        <attr name="sUseRipple" format="boolean" />

        <attr name="sBackgroundDrawableRes" format="reference" />

        <!--是否使用shape设置圆角及触摸反馈 设为true之后才能使用以下属性-->
        <attr name="sUseShape" format="boolean" />
        <!--  填充色-->

        <attr name="sShapeSolidColor" format="color" />

        <attr name="sShapeSelectorPressedColor" format="color" />
        <attr name="sShapeSelectorNormalColor" format="color" />

        <!-- 圆角半径 -->
        <attr name="sShapeCornersRadius" format="dimension" />

        <attr name="sShapeCornersTopLeftRadius" format="dimension" />
        <attr name="sShapeCornersTopRightRadius" format="dimension" />
        <attr name="sShapeCornersBottomLeftRadius" format="dimension" />
        <attr name="sShapeCornersBottomRightRadius" format="dimension" />

        <!--边框宽度-->
        <attr name="sShapeStrokeWidth" format="dimension" />
        <attr name="sShapeStrokeDashWidth" format="dimension" />
        <attr name="sShapeStrokeDashGap" format="dimension" />
        <attr name="sShapeStrokeColor" format="color" />

    </declare-styleable>

    <declare-styleable name="ClearEditText">
        <!--清除按钮的图片资源-->
        <attr name="cet_clearIcon" format="reference" />
        <!--清除按钮的图片尺寸-->
        <attr name="cet_clearIconSize" format="dimension" />
    </declare-styleable>
    <attr name="ClearEditTextStyle" format="reference" />

    <!-- PasswordEditText -->
    <declare-styleable name="PasswordEditText">
        <!--显示密码的图标资源-->
        <attr name="pet_iconShow" format="reference" />
        <!--隐藏密码的图标资源-->
        <attr name="pet_iconHide" format="reference" />
        <!--是否触摸显示密码-->
        <attr name="pet_hoverShowsPw" format="boolean" />
        <!--是否使用系统默认字体-->
        <attr name="pet_nonMonospaceFont" format="boolean" />
        <!--图标是否设置透明度, 默认是true-->
        <attr name="pet_enableIconAlpha" format="boolean" />
    </declare-styleable>
    <attr name="PasswordEditTextStyle" format="reference" />


    <!--============================Banner=========================================-->

    <!-- BaseBanner -->
    <declare-styleable name="BaseBanner">
        <!-- Banner高宽比,范围0-1,默认0.5(for LoopViewPager)-->
        <attr name="bb_scale" format="float" />

        <!--for LoopViewPager start-->
        <!-- 是否支持循环,默认true-->
        <attr name="bb_isLoopEnable" format="boolean" />
        <!-- 滚动延时,默认5秒-->
        <attr name="bb_delay" format="integer" />
        <!-- 滚动间隔,默认5秒-->
        <attr name="bb_period" format="integer" />
        <!-- 是否支持自动滚动,默认true-->
        <attr name="bb_isAutoScrollEnable" format="boolean" />
        <!--for LoopViewPager end-->

        <!-- 设置底部背景条颜色,默认透明 -->
        <attr name="bb_barColor" format="color" />
        <!-- 设置当滚动到最后一个item时是否显示底部背景条 -->
        <attr name="bb_isBarShowWhenLast" format="boolean" />
        <!-- 设置底部背景条padding,单位dp -->
        <attr name="bb_barPaddingLeft" format="dimension" />
        <attr name="bb_barPaddingTop" format="dimension" />
        <attr name="bb_barPaddingRight" format="dimension" />
        <attr name="bb_barPaddingBottom" format="dimension" />
        <!-- 设置标题文字颜色,默认"#ffffff" -->
        <attr name="bb_textColor" format="color" />
        <!-- 设置标题文字大小,单位sp,默认14sp -->
        <attr name="bb_textSize" format="dimension" />
        <!-- 设置是否显示标题,默认true  -->
        <attr name="bb_isTitleShow" format="boolean" />
        <!-- 设置是否显示指示器,默认true  -->
        <attr name="bb_isIndicatorShow" format="boolean" />
        <!-- 设置指示器位置.默认Gravity.CENTER时文字不显示-->
        <attr name="bb_indicatorGravity" format="enum">
            <enum name="CENTER" value="17" />
            <enum name="LEFT" value="3" />
            <enum name="RIGHT" value="5" />
        </attr>
    </declare-styleable>

    <!-- BaseIndicatorBanner -->
    <declare-styleable name="BaseIndicatorBanner">
        <!-- 设置指示器样式 -->
        <attr name="bb_indicatorStyle" format="enum">
            <!--图片资源-->
            <enum name="DRAWABLE_RESOURCE" value="0" />
            <!--矩形-->
            <enum name="CORNER_RECTANGLE" value="1" />
        </attr>
        <!-- 设置指示器宽度,单位dp,默认6dp -->
        <attr name="bb_indicatorWidth" format="dimension" />
        <!-- 设置指示器高度,单位dp,默认6dp -->
        <attr name="bb_indicatorHeight" format="dimension" />
        <!-- 设置指示器间距,单位dp,默认6dp -->
        <attr name="bb_indicatorGap" format="dimension" />

        <!-- 设置指示器选中颜色,默认"#ffffff" -->
        <attr name="bb_indicatorSelectColor" format="color" />
        <!-- 设置指示器未选中颜色,默认"#88ffffff"  -->
        <attr name="bb_indicatorUnselectColor" format="color" />
        <!-- 设置指示器圆角弧度,单位dp,默认3dp -->
        <attr name="bb_indicatorCornerRadius" format="dimension" />

        <!-- 设置指示器选中drawable资源 -->
        <attr name="bb_indicatorSelectRes" format="reference" />
        <!-- 设置指示器未选中drawable资源 -->
        <attr name="bb_indicatorUnselectRes" format="reference" />
    </declare-styleable>

    <!--RecyclerView实现的banner-->
    <declare-styleable name="RecyclerViewBannerBase">
        <attr name="rvbb_interval" format="integer" />
        <attr name="rvbb_showIndicator" format="boolean" />
        <attr name="rvbb_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="rvbb_autoPlaying" format="boolean" />
        <attr name="rvbb_indicatorSelectedSrc" format="reference" />
        <attr name="rvbb_indicatorUnselectedSrc" format="reference" />
        <attr name="rvbb_indicatorSize" format="dimension" />
        <attr name="rvbb_indicatorSelectedColor" format="color" />
        <attr name="rvbb_indicatorUnselectedColor" format="color" />
        <attr name="rvbb_indicatorSpace" format="dimension" />
        <attr name="rvbb_indicatorMarginLeft" format="dimension" />
        <attr name="rvbb_indicatorMarginRight" format="dimension" />
        <attr name="rvbb_indicatorMarginBottom" format="dimension" />
        <attr name="rvbb_indicatorGravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="BannerLayout">
        <!--轮播间隔，单位ms，默认是4000ms-->
        <attr name="bl_interval" format="integer" />
        <!--是否显示轮播索引，默认是true-->
        <attr name="bl_showIndicator" format="boolean" />
        <!--轮播的方向，默认是horizontal-->
        <attr name="bl_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <!--是否是自动轮播，默认是true-->
        <attr name="bl_autoPlaying" format="boolean" />
        <!--轮播索引选中的效果-->
        <attr name="bl_indicatorSelectedSrc" format="reference" />
        <!--轮播索引未选中的效果-->
        <attr name="bl_indicatorUnselectedSrc" format="reference" />
        <!--轮播索引的大小，默认5dp-->
        <attr name="bl_indicatorSize" format="dimension" />
        <!--轮播索引选中的颜色-->
        <attr name="bl_indicatorSelectedColor" format="color" />
        <!--轮播索引未选中的颜色-->
        <attr name="bl_indicatorUnselectedColor" format="color" />
        <!--索引器之间的间隔，默认4dp-->
        <attr name="bl_indicatorSpace" format="dimension" />
        <!--索引器的左侧边距，默认16dp-->
        <attr name="bl_indicatorMarginLeft" format="dimension" />
        <!--索引器的右侧边距，默认0dp-->
        <attr name="bl_indicatorMarginRight" format="dimension" />
        <!--索引器的底部边距，默认11dp-->
        <attr name="bl_indicatorMarginBottom" format="dimension" />
        <!--索引器的对齐方式，默认left-->
        <attr name="bl_indicatorGravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <!--轮播图之间的间距，默认是10dp-->
        <attr name="bl_itemSpace" format="dimension" />
        <!--图片缩放系数，默认是1.2F-->
        <attr name="bl_centerScale" format="float" />
        <!--轮播速度，默认是1.0F-->
        <attr name="bl_moveSpeed" format="float" />
